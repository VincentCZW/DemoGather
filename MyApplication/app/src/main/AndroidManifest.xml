<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.annotation_tool.demogather" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>

    <application
        android:name="com.example.annotation_tool.demogather.application.DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.annotation_tool.demogather.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.annotation_tool.demogather.OtherActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.activity.TouchEventTestActivity"/>
        <activity android:name=".fresco.FrescoLoadImageListActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.muti_thread.MutiThreadTestMainActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.muti_thread.SychronizedTestActivity" />
        <activity android:name="com.example.annotation_tool.demogather.muti_thread.WaitNotifyActivity"/>
        <activity android:name=".retrofit.RetrofitActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.diy_view.DiyViewMainActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.anr.ANRMainActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.java.JavaMainActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.muti_thread.ConsumerProducerActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.hotfix.HotFixActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.butter_knife.ButterKnifeMainActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.OpenStructureMainActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.annotation.AnnotationMainActivity"/>
        <activity android:name="com.example.annotation_tool.demogather.annotation.CompileAnnotationMainActivity"/>
        <activity android:name=".event_bus.EventBusMainActivity" />
        <activity android:name=".event_bus.EventBusSecondActivity" />
        <activity android:name=".android.StorageDirMainActivity"/>
        <activity android:name=".AndroidMainActivity"/>
    </application>

</manifest>
